// schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      Int      @id @default(autoincrement()) // Alterado para Int e autoincremento
  email   String   @unique
  name    String
  age     Int
}
